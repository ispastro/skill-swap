{
  "name": "skillswap",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "test": "jest --detectOpenHandles --forceExit",
    "start": "nodemon src/server.js",
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start:prod": "node dist/server.js",
    "type-check": "tsc --noEmit"
  },
  "keywords": [],
  "author": "",
  "type": "module",
  "license": "ISC",
  "description": "",
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "testEnvironment": "node",
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "useESM": true
        }
      ],
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "dependencies": {
    "@prisma/client": "^6.12.0",
    "@supabase/supabase-js": "^2.55.0",
    "@upstash/redis": "^1.35.2",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.2.1",
    "fuse.js": "^7.1.0",
    "fuzzysort": "^3.1.0",
    "helmet": "^8.1.0",
    "joi": "^18.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "3.7",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.1.10",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "prisma": "^6.12.0",
    "redis": "^5.6.1",
    "socket.io": "^4.8.1",
    "string-similarity": "^4.0.4",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.3.0",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.17",
    "@types/string-similarity": "^4.0.2",
    "@types/supertest": "^6.0.3",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-mock": "^29.7.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}